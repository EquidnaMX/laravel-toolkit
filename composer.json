{
  "name": "equidna/laravel-toolkit",
  "description": "Multi-context Laravel helpers, middleware, and utilities.",
  "version": "1.0.3",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Ruelas",
      "email": "gruelasjr@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^11.21 || ^12.0",
    "laravel/helpers": "^1.7",
    "illuminate/support": "^11.21 || ^12.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "*",
    "larastan/larastan": "*",
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "Equidna\\Toolkit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Equidna\\Toolkit\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Equidna\\Toolkit\\Providers\\EquidnaLaravelToolkitServiceProvider"
      ]
    }
  }
}
